(ls=="$35,000 - $52,000") && return(1)
(ls=="$52,000 - $70,000") && return(2)
(ls=="$70,000 - $87,000") && return(3)
(ls=="$87,000 - $105,000") && return(4)
(ls=="More than $105,000") && return(5)
(ls=="Prefer not to answer") && return(NA)
})
table(num_income)
table(d$Income)
